{
    "builds": [{
        "src": "homeassistant_renamer/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "homeassistant_renamer/wsgi.py"
        }
    ]
}
{
    "functions": {
        "app/api/**/*": {
            "maxDuration": 60 // All functions can run for a maximum of 60 seconds
        }
    }
}
