{
    "functions": {
        "homeassistant_renamer/wsgi.py": {
            "runtime": "@vercel/python",
            "maxDuration": 60,
            "memory": 1024,  // Optional, specify the memory in MB
            "maxLambdaSize": "15mb"  // Optional, specify the max lambda size
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "homeassistant_renamer/wsgi.py"
        }
    ]
}
