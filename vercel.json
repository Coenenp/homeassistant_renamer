{
    "functions": {
        "homeassistant_renamer/wsgi.py": {
            "runtime": "@vercel/python@3.12",
            "maxDuration": 60,
            "memory": 1024,
            "includeFiles": "homeassistant_renamer/**/*",
            "excludeFiles": "homeassistant_renamer/tests/*"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "homeassistant_renamer/wsgi.py"
        }
    ]
}
